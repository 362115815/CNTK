<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <BuildDir>$(SolutionDir)..\build</BuildDir>
    <BuildScriptsDir>$(SolutionDir)\build_scripts</BuildScriptsDir>
    <NugetDir>$(SolutionDir)..\..\nuget</NugetDir>
    <CommonDir>$(SolutionDir)common</CommonDir>
    <DsLibDir>$(SolutionDir)dataset\dataset_lib</DsLibDir>
    <CaffeRepositoryRoot></CaffeRepositoryRoot>
    <CntkRepositoryRoot></CntkRepositoryRoot>
    <!-- Macros that control building projects -->
    <BuildCommonCmdLineParser>true</BuildCommonCmdLineParser>
    <BuildCommonStandalone>true</BuildCommonStandalone>
    <BuildCommonPlatform>true</BuildCommonPlatform>
    <BuildCommonProtobufWrapper>true</BuildCommonProtobufWrapper>
    <BuildCaffeDynLib>false</BuildCaffeDynLib>
    <CaffexDnnxBuild>false</CaffexDnnxBuild>
    <BuildDatatsetLib>true</BuildDatatsetLib>
    <BuildDatatsetTool>true</BuildDatatsetTool>
    <BuildCntkToCaffe>false</BuildCntkToCaffe>
    <!-- If BuildCaffeModelEditor is changed to true then CaffeRepositoryRoot must be set -->
    <BuildCaffeModelEditor>false</BuildCaffeModelEditor>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(BuildDir)\$(Platform)\$(Configuration)\</OutDir>
    <IntDir>$(BuildDir)\Int\$(ProjectName)\$(Platform)\$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <WarningLevel>Level4</WarningLevel>
      <TreatWarningAsError>true</TreatWarningAsError>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>